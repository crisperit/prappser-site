{
    "name": "Countdown",
    "version": "1.0.0",
    "description": "Create a countdown to your goal",
    "parameters": [
        {
            "name": "goalDate",
            "hidden": false,
            "defaultValue": "2024-06-20",
            "type": "date",
            "validators": [
                {
                    "type": "required"
                }
            ],
            "description": "Counting down to this date"
        },
        {
            "name": "goalName",
            "hidden": false,
            "defaultValue": "your goal",
            "type": "text",
            "validators": [
                {
                    "type": "required"
                }
            ],
            "description": "Name of your goal"
        }
    ]
}